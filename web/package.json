{
  "name": "dzq-default-web",
  "version": "0.0.1-beta.3",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=dzq_error NODE_ENV=development dzq dev -port 9527",
    "build": "cross-env DEBUG=dzq_error NODE_ENV=production dzq build",
    "build:custom": "cross-env DEBUG=dzq_error DISCUZ_CONFIG_HOST=https://discuz.chat NODE_ENV=production dzq build",
    "build:analyze": "cross-env DEBUG=dzq_error ANALYZE=true NODE_ENV=production dzq build",
    "static": "cross-env DEBUG=dzq_error NODE_ENV=production dzq build -s",
    "start": "cross-env DEBUG=dzq_error NODE_ENV=production DZQ_SSR_HOST=https://discuz.chat dzq start -H 127.0.0.1 -port 9527",
    "start:static": "cross-env DEBUG=dzq_error NODE_ENV=production dzq start -H 127.0.0.1 -port 9527 --no-ssr",
    "update": "dzq update",
    "sls:create": "npm run build && dzq sls",
    "sls:build": "dzq sls --no-create -z -c",
    "sls:publish": "cd ./.sls && sls deploy"
  },
